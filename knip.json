{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/index.ts", "src/commands/**/*.ts", "bin/*.js"],
  "project": ["src/**/*.ts"],
  "ignore": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "dist/**",
    "coverage/**",
    "src/daemon/**"
  ],
  "ignoreDependencies": ["tsx", "@cspell/dict-he", "@types/uuid"],
  "ignoreExportsUsedInFile": true,
  "ignoreWorkspaces": []
}
