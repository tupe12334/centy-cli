{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/index.ts",
    "src/commands/**/*.ts",
    "src/tui/index.ts",
    "bin/*.js"
  ],
  "project": ["src/**/*.ts", "src/**/*.tsx"],
  "ignore": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "dist/**",
    "coverage/**",
    "src/daemon/**"
  ],
  "ignoreDependencies": ["tsx", "@cspell/dict-he"],
  "ignoreExportsUsedInFile": true,
  "ignoreWorkspaces": []
}
